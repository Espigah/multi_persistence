{
  "name": "products-server",
  "version": "1.0.0",
  "description": "Create a minimal app with docker-compose and multiple storage destinations",
  "main": "index.js",
  "scripts": {
    "test": "nyc ./node_modules/.bin/mocha --require @babel/register ./*.spec.js",
    "reload": "npx pm2 reload ecosystem.config.js --env production",
    "mocha": "./node_modules/.bin/mocha",
    "build": "npx babel -w src/  -d build/  && npx babel -w index.js app.js database.config.js -d build/ && cp adapters.config.json ",
    "nodemon": "./node_modules/.bin/nodemon",
    "start": "nodemon --exec babel-node ./index.js",
    "json": "npx json-server --watch json-server/db.json",
    "redis": "redis-commander",
    "docker": "npm run test && docker build -t fabriciogoncalves/stock_server_node_express .",
    "docker:run": "docker run -it --rm  -p 4321:4321  fabriciogoncalves/stock_server_node_express",
    "docker:test": "docker build -f Dockerfile.tests.dockerfile -t fabriciogoncalves/stock_server_node_express_test .",
    "docker:test:run": "docker run -it --rm  fabriciogoncalves/stock_server_node_express_test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@std/esm": "^0.26.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",  
    "mongoose": "^5.8.3",
    "node-fetch": "^2.6.0",
    "pg-promise": "^9.3.6",
    "pm2": "^4.2.1",
    "pouchdb-node": "^7.1.1",
    "redis": "^2.8.0",
    "redis-commander": "^0.6.6",
    "require-all": "^3.0.0",
    "save-dev": "0.0.1-security",
    "yargs": "^14.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "sinon": "^7.5.0",
    "json-server": "^0.15.1"
  }
}
